<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="http://www.demandware.com/xml/impex/catalog/2006-10-31" catalog-id="storefront-catalog-m-en-allCS">
    <header>
        <image-settings>
            <internal-location base-path="/"/>
            <view-types>
                <view-type>large</view-type>
                <view-type>medium</view-type>
                <view-type>small</view-type>
                <view-type>swatch</view-type>
            </view-types>
            <alt-pattern>${productname}</alt-pattern>
            <title-pattern>${productname}</title-pattern>
        </image-settings>
    </header>

    <category category-id="stockClearance">
        <display-name xml:lang="x-default">stockClearance</display-name>
        <description xml:lang="x-default">stockClearance</description>
        <online-flag>true</online-flag>
        <parent>root</parent>
        <template/>
        <page-attributes/>
        <custom-attributes>
            <custom-attribute attribute-id="enableCompare">false</custom-attribute>
            <custom-attribute attribute-id="showInMenu">false</custom-attribute>
            <custom-attribute attribute-id="slotBannerImage">clear4.jpg</custom-attribute>
        </custom-attributes>
    </category>
</catalog>
